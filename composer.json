{
    "name": "AdamToms/RoyalMailV4",
    "description": "RoyalMail V4 Api generated by swagger",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "rest",
        "api",
        "RoyalMail",
        "RoyalMailV4"
    ],
    "homepage": "https://github.com/AdamToms/royalmailv4",
    "license": "proprietary",
    "authors": [
        {
            "name": "RoyalMail",
            "homepage": "https://royalmail.proshipping.net/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "AdamToms\\RoyalMailV4\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "AdamToms\\RoyalMailV4\\" : "test/" }
    }
}
